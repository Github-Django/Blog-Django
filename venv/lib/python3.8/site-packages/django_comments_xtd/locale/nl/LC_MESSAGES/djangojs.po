#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "This field is required."
msgstr "Dit veld is verplicht."

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "Your comment"
msgstr "Uw reactie"

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "name"
msgstr "naam"

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "Name"
msgstr "Naam"

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "mail address"
msgstr "e-mailadres"

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "Required for comment verification."
msgstr "Verplicht voor verificatie per e-mail."

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "Mail"
msgstr "Mail"

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "url your name links to (optional)"
msgstr "URL waar uw naam naar linkt (optioneel)."

