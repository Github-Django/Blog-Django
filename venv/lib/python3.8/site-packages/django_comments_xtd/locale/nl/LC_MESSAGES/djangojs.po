msgid "remove comment"
msgstr "verwijder reactie"

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "I like it"
msgstr "Vind ik leuk!"

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "I dislike it"
msgstr "Vind ik niet leuk!"

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "Reply"
msgstr "Beantwoorden..."

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
#, javascript-format
msgid "%s comment."
msgid_plural "%s comments."
msgstr[0] "%s reactie."
msgstr[1] "%s reacties."

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "Only registered users can post comments."
msgstr "Alleen geregistreerde gebruikers kunnen opmerkingen plaatsen."

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
msgid "Comments are disabled for this article."
msgstr ""

#: static/django_comments_xtd/js/plugin-2.7.1.js:1
#, javascript-format
msgid "There is %s new comment."
msgid_plural "There are %s new comments."
msgstr[0] "Er is %s nieuw reactie."
msgstr[1] "Er zijn %s reacties."
