#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "This field is required."
msgstr "Questo campo è obbligatorio"

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "Your comment"
msgstr "Tuo commento"

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "name"
msgstr "nome"

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "Name"
msgstr "Nome"

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "mail address"
msgstr "indirizzo mail"

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "Required for comment verification."
msgstr "Necessario per verificare il commento."

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "Mail"
msgstr "Mail"

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "url your name links to (optional)"
msgstr "url collegato al tuo nome (facoltativo)"

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
msgid "Link"
msgstr "Link"

#: .\static\django_comments_xtd\js\plugin-2.8.3.js:1
